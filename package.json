    "@angular/router": "~12.2.13",
    "angular-cli-ghpages": "^0.6.2",
    "bootstrap": "^4.4.1",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "ngx-markdown": "^12.0.1",
    "ngx-quill": "^7.3.11",
    "popper.js": "^1.16.1",
