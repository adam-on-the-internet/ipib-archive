{
  "name": "ipib-archive",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "setup-ghpages": "ng add angular-cli-ghpages",
    "ghpd": "ng deploy --base-href https://adam-on-the-internet.github.io/ipib-archive/",
    "start": "node server.js",
    "serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.44",
    "e2e": "ng e2e --webdriver-update=false",
    "build-for-prod": "ng build --configuration production --base-href=/projects/ipib-archive/",
    "prod-deploy": "scp -r dist/* callanan@ftp.callananconcepts.club:/home1/callanan/public_html/projects/",
    "full-deploy": "npm run build-for-prod && npm run prod-deploy",
    "postinstall": "ng build --aot --configuration production"
  },
  "private": true,
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "dependencies": {
    "@angular/animations": "~12.2.13",
